<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="managerListForDepartment"
  Version="11.1.1.61.92"
  InheritPersonalization="true"
  SelectList="Emp.EMPNO, 
       Emp.DEPTNO, 
       Emp.ENAME"
  FromList="EMP Emp"
  Where="(DEPTNO = :CurrentDept OR MGR IS NULL) AND EMPNO != :CurrentEmp"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full"
  UseGlueCode="false">
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="sharedmodel.managerListForDepartment_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="CurrentDept"
    Kind="where"
    Type="java.lang.String"/>
  <Variable
    Name="CurrentEmp"
    Kind="where"
    Type="java.lang.String"/>
  <EntityUsage
    Name="Emp"
    Entity="model.Emp"/>
  <ViewAttribute
    Name="Empno"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Empno"
    EntityUsage="Emp"
    AliasName="EMPNO"/>
  <ViewAttribute
    Name="Deptno"
    PrecisionRule="true"
    EntityAttrName="Deptno"
    EntityUsage="Emp"
    AliasName="DEPTNO"/>
  <ViewAttribute
    Name="Ename"
    PrecisionRule="true"
    EntityAttrName="Ename"
    EntityUsage="Emp"
    AliasName="ENAME"/>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
